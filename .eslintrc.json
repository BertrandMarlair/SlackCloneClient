{
    "plugins": [
        "react-hooks"
    ],
    "extends": "react-app",
    "rules": {
        "semi": ["error", "never"],
        "quotes": ["error", "single", {
            "allowTemplateLiterals": true
        }],
        "jsx-quotes": ["error", "prefer-double"],
        "no-extra-semi": 0,
        "no-console": 0,
        "react-hooks/rules-of-hooks": "error"
    }
}